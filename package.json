{
  "name": "cyanmaple",
  "version": "2.0.3",
  "description": "A library based on lodash and vue 2.6+",
  "private": false,
  "main": "CMUI/lib/index.common.js",
  "scripts": {
    "start": "npm run cyan && npm run theme && vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "cyan": "gulp sass",
    "maple": "cross-env NODE_ENV=production && node ./build/rollup.maple",
    "theme": "gulp theme",
    "doc": "vuepress dev doc",
    "build": "cross-env NODE_ENV=production rimraf ./CMUI && node ./build/rollup.build && node ./build/cli.build.js"
  },
  "files": [
    "src/",
    "CMUI/"
  ],
  "keywords": [
    "vue",
    "CyanMaple",
    "ui",
    "mobile"
  ],
  "author": "qqkillqq",
  "license": "MIT",
  "style": "CMUI/style/CMUI.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/tgoufe/CyanMapleDesign"
  },
  "homepage": "http://www.bingshangroup.com",
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.0",
    "@vue/cli-plugin-eslint": "^3.12.0",
    "@vue/cli-service": "^3.12.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "cross-env": "^6.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "gulp": "^4.0.2",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "inquirer": "^7.0.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.2",
    "postcss-preset-env": "^6.7.0",
    "postcss-simple-vars": "^5.0.2",
    "rollup": "^1.23.1",
    "rollup-plugin-alias": "^2.0.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-img": "^1.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-vue": "^5.0.1",
    "sass-loader": "^7.3.1",
    "tasksfile": "^5.1.0",
    "vue-template-compiler": "^2.6.10"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "dayjs": "^1.8.16",
    "swiper": "^5.0.4",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "iOS >= 8",
    "Android >= 4"
  ]
}
